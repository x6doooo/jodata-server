[{
  "name"             : "jodata-server",
  "cwd"              : "./",
  "script"           : "index.js",
  "exec_mode"        : "cluster",
  "instances"        : 2,
  "node_args"        : ["--harmony"],
  "log_date_format"  : "YYYY-MM-DDTHH:mm:ssZ",
  "error_file"       : "/home/fe/log/jodata-server/log.stderr.log",
  "out_file"         : "/home/fe/log/jodata-server/log.stdout.log",
  "pid_file"         : "../jodata-server.pid",
  "merge_logs"       : true,
  "max_memory_restart" : "500M",
  "env": {
    "NODE_ENV": "prod"
  }
}]